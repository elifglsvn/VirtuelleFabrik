Create Table produkt
(
produktID int not null,
produktname varchar(30),
preis float,
listenID int,
Primary key (produktID),
FOREIGN KEY (listenID) REFERENCES bestellliste(listenID)
);

produktID, produktname, menge, auID, lagID
1001, Zylinder, 100, 10001, 4
1002, Keilriemen, 150, 10002, 2
1003, Zündkerze, 180, 10003, 5
1004, Stoßdämpfer, 300, 10004, 4
1005, Bremsscheibe, 200, 10005, 2
1006, Bremssattel, 250, 10006, 2
