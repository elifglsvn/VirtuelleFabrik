Create Table bestellliste
(
listenID int not null,
produktname varchar(30),
bestellmenge int,
bestellID int,
Primary key (listenID, produktname),
FOREIGN KEY (bestellID) REFERENCES Bestellung(bestellID)
);

listenID,produktname,bestellmenge,bestellID
1,Zylinder,10,11000
2,Keilriemen,10,11001
3,Zündkerze,10,11002
4,Stoßdämpfer,20,11003
5,Bremsscheibe,20,11004
6,Bremssattel,15,11005
