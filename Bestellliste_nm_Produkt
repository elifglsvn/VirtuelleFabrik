
Create Table bestellliste_nm_produkt
(
listenID int not null,
produktID int,
Primary key (listenID, produktID),
FOREIGN KEY (listenID) REFERENCES bestellliste(listenID),
FOREIGN KEY (produktID) REFERENCES produkt(produktID)
)

